<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">
    <servlet>
        <servlet-name>SortSimulatorController</servlet-name>
        <servlet-class>controller.SortSimulatorController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SortSimulatorController</servlet-name>
        <url-pattern>/sortsimulator</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SortController</servlet-name>
        <servlet-class>controller.SortController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SortController</servlet-name>
        <url-pattern>/sort</url-pattern>
    </servlet-mapping>

</web-app>
